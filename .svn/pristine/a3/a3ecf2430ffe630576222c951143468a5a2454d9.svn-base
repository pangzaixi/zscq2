<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
    <meta name="keywords" content="天地图"/>
    <title>天地图－地图API－范例－聚合Marker</title>
    <script type="text/javascript" src="http://api.tianditu.gov.cn/api?v=4.0&tk=661c60fd0d30694200e0fd7b25bd744a"></script>
    <style type="text/css">body,html{width:100%;height:98%;margin:0;font-family:"Microsoft YaHei"}#mapDiv{width:100%;height:98%}input,b,p{margin-left:5px;font-size:14px}</style>
    <script>
        var zoom = 5;
        var map;
        function onLoad() {
            map = new T.Map('mapDiv', {
                attributionControl: false,
                inertia: false
            });
            map.centerAndZoom(new T.LngLat(116.40969, 37.43997405227057), zoom);

            //创建图片对象iconUrl: "http://api.tianditu.gov.cn/img/map/markerA.png",
            var icon = new T.Icon({
                iconUrl: "http://172.20.15.97:8080/pjgl/image/car.png",
                iconSize: new T.Point(19, 27),
                iconAnchor: new T.Point(10, 25)
            });
            
            var arrayObj = new Array();
            for (var i = 0; i < 500; i++) {
                var marker = new T.Marker(new T.LngLat(Math.random() * 40 + 85, Math.random() * 30 + 21),  {icon: icon});
                arrayObj.push(marker);
            }
            var markers = new T.MarkerClusterer(map, {markers: arrayObj});
        }

    </script>
</head>
<body onLoad="onLoad()">
<div id="mapDiv" style="position:absolute;width:100%; height:98%"></div>
<p></p>
</body>
</html>
